CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE CHANGELOG(CHANGE_NUMBER INTEGER NOT NULL,DELTA_SET VARCHAR(10) NOT NULL,START_DT TIMESTAMP NOT NULL,COMPLETE_DT TIMESTAMP,APPLIED_BY VARCHAR(100) NOT NULL,DESCRIPTION VARCHAR(500) NOT NULL,CONSTRAINT PKCHANGELOG PRIMARY KEY(CHANGE_NUMBER,DELTA_SET))
CREATE MEMORY TABLE SEC_GROUP(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,CODE VARCHAR(50) NOT NULL,NAME VARCHAR(50) NOT NULL,ALLOW_MANAGE INTEGER NOT NULL,PARENT_ID BIGINT,CREATOR VARCHAR(50) NOT NULL,CREATE_TIME TIMESTAMP NOT NULL,UPDATE_TIME TIMESTAMP,STATUS INTEGER NOT NULL,SEQ INTEGER NOT NULL,DESCN VARCHAR(200),CONSTRAINT PK_SEC_GROUP PRIMARY KEY(ID),CONSTRAINT FK_SEC_GROUP_PARENT FOREIGN KEY(PARENT_ID) REFERENCES SEC_GROUP(ID))
CREATE MEMORY TABLE SEC_USER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,USERNAME VARCHAR(50) NOT NULL,PASSWORD VARCHAR(50) NOT NULL,CREATOR VARCHAR(50) NOT NULL,CREATE_TIME TIMESTAMP NOT NULL,UPDATE_TIME TIMESTAMP,STATUS INTEGER NOT NULL,GROUP_ID BIGINT NOT NULL,LOGO VARCHAR(200) NOT NULL,DESCN VARCHAR(200),CONSTRAINT PK_SEC_USER PRIMARY KEY(ID),CONSTRAINT FK_SEC_USER_GROUP FOREIGN KEY(GROUP_ID) REFERENCES SEC_GROUP(ID))
CREATE MEMORY TABLE SEC_ROLE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,NAME VARCHAR(50) NOT NULL,CREATOR VARCHAR(50) NOT NULL,CREATE_TIME TIMESTAMP NOT NULL,UPDATE_TIME TIMESTAMP,STATUS INTEGER NOT NULL,GROUP_ID BIGINT NOT NULL,LOGO VARCHAR(200) NOT NULL,DESCN VARCHAR(200),CONSTRAINT PK_SEC_ROLE PRIMARY KEY(ID),CONSTRAINT FK_SEC_ROLE_GROUP FOREIGN KEY(GROUP_ID) REFERENCES SEC_GROUP(ID))
CREATE MEMORY TABLE SEC_RESC(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,RES_TYPE VARCHAR(100) NOT NULL,RES_STRING VARCHAR(200) NOT NULL,CREATOR VARCHAR(50) NOT NULL,CREATE_TIME TIMESTAMP NOT NULL,UPDATE_TIME TIMESTAMP,STATUS INTEGER NOT NULL,PRIORITY INTEGER NOT NULL,GROUP_ID BIGINT NOT NULL,DESCN VARCHAR(200),CONSTRAINT PK_SEC_RESC PRIMARY KEY(ID),CONSTRAINT FK_SEC_RESC_GROUP FOREIGN KEY(GROUP_ID) REFERENCES SEC_GROUP(ID))
CREATE MEMORY TABLE SEC_USER_ROLE(USER_ID BIGINT NOT NULL,ROLE_ID BIGINT NOT NULL,CONSTRAINT PK_SEC_USER_ROLE PRIMARY KEY(USER_ID,ROLE_ID),CONSTRAINT FK_SEC_USER_ROLE_USER FOREIGN KEY(USER_ID) REFERENCES SEC_USER(ID),CONSTRAINT FK_SEC_USER_ROLE_ROLE FOREIGN KEY(ROLE_ID) REFERENCES SEC_ROLE(ID))
CREATE MEMORY TABLE SEC_ROLE_RESC(ROLE_ID BIGINT NOT NULL,RESC_ID BIGINT NOT NULL,CONSTRAINT PK_SEC_ROLE_RESC PRIMARY KEY(ROLE_ID,RESC_ID),CONSTRAINT FK_SEC_ROLE_RESC_ROLE FOREIGN KEY(ROLE_ID) REFERENCES SEC_ROLE(ID),CONSTRAINT FK_SEC_ROLE_RESC_RESC FOREIGN KEY(RESC_ID) REFERENCES SEC_RESC(ID))
CREATE MEMORY TABLE SEC_GROUP_ROLE(GROUP_ID BIGINT NOT NULL,ROLE_ID BIGINT NOT NULL,CONSTRAINT PK_SEC_GROUP_ROLE PRIMARY KEY(GROUP_ID,ROLE_ID),CONSTRAINT FK_SEC_GROUP_ROLE_GROUP FOREIGN KEY(GROUP_ID) REFERENCES SEC_GROUP(ID),CONSTRAINT FK_SEC_GROUP_ROLE_ROLE FOREIGN KEY(ROLE_ID) REFERENCES SEC_ROLE(ID))
CREATE MEMORY TABLE SEC_GROUP_RESC(GROUP_ID BIGINT NOT NULL,RESC_ID BIGINT NOT NULL,CONSTRAINT PK_SEC_GROUP_RESC PRIMARY KEY(GROUP_ID,RESC_ID),CONSTRAINT FK_SEC_GROUP_RESC_GROUP FOREIGN KEY(GROUP_ID) REFERENCES SEC_GROUP(ID),CONSTRAINT FK_SEC_GROUP_RESC_RESC FOREIGN KEY(RESC_ID) REFERENCES SEC_RESC(ID))
CREATE MEMORY TABLE SEC_GROUP_USER(GROUP_ID BIGINT NOT NULL,USER_ID BIGINT NOT NULL,CONSTRAINT PK_SEC_GROUP_USER PRIMARY KEY(GROUP_ID,USER_ID),CONSTRAINT FK_SEC_GROUP_USER_GROUP FOREIGN KEY(GROUP_ID) REFERENCES SEC_GROUP(ID),CONSTRAINT FK_SEC_GROUP_USER_USER FOREIGN KEY(USER_ID) REFERENCES SEC_USER(ID))
CREATE MEMORY TABLE CMS_SITE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,NAME VARCHAR(50),GROUP_ID BIGINT,DESCN VARCHAR(50),CONSTRAINT PK_CMS_SITE PRIMARY KEY(ID),CONSTRAINT FK_CMS_SITE_GROUP FOREIGN KEY(GROUP_ID) REFERENCES SEC_GROUP(ID))
CREATE MEMORY TABLE CMS_CATEGORY(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,NAME VARCHAR(50),SITE_ID BIGINT,SEQ INTEGER,STATUS INTEGER,DESCN VARCHAR(200),CONSTRAINT PK_CMS_CATEGORY PRIMARY KEY(ID),CONSTRAINT FK_CMS_CATEGORY_SITE FOREIGN KEY(SITE_ID) REFERENCES CMS_SITE(ID))
CREATE MEMORY TABLE CMS_CONTENT(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,TITLE VARCHAR(50),CATEGORY_ID BIGINT,STATUS INTEGER,CREATOR VARCHAR(50),CREATE_TIME TIMESTAMP,UPDATE_TIME TIMESTAMP,CONTENT VARCHAR(2000),CONSTRAINT PK_CMS_CONTENT PRIMARY KEY(ID),CONSTRAINT FK_CMS_CONTENT_CATEGORY FOREIGN KEY(CATEGORY_ID) REFERENCES CMS_CATEGORY(ID))
CREATE MEMORY TABLE CMS_ATTACH(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,NAME VARCHAR(50),PATH VARCHAR(200),CONTENT_ID BIGINT,CREATE_TIME TIMESTAMP,DESCN VARCHAR(200),CONSTRAINT PK_CMS_ATTACH PRIMARY KEY(ID),CONSTRAINT FK_CMS_ATTACH_CONTENT FOREIGN KEY(CONTENT_ID) REFERENCES CMS_CONTENT(ID))
ALTER TABLE SEC_GROUP ALTER COLUMN ID RESTART WITH 5
ALTER TABLE SEC_USER ALTER COLUMN ID RESTART WITH 6
ALTER TABLE SEC_ROLE ALTER COLUMN ID RESTART WITH 7
ALTER TABLE SEC_RESC ALTER COLUMN ID RESTART WITH 37
ALTER TABLE CMS_SITE ALTER COLUMN ID RESTART WITH 4
ALTER TABLE CMS_CATEGORY ALTER COLUMN ID RESTART WITH 1
ALTER TABLE CMS_CONTENT ALTER COLUMN ID RESTART WITH 1
ALTER TABLE CMS_ATTACH ALTER COLUMN ID RESTART WITH 1
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
